<h1>New Student</h1>
<form action="/students/create" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <h2>First Name:</h2><input type="text" name="first_name">
    <h2>Last Name:</h2><input type="text" name="last_name">
    <h2>Email:</h2><input type="text" name="email">
    <select name='dojo_id'>
        
        <% @dojos.each do |dojo|%>
            <option value='<%= dojo.id%>'><%= dojo.branch %></option>
        <% end %>
        </select>
        <input type='submit' value='Add Student'>
</form>


<%= yield %>

