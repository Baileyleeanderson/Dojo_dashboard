<h1><%= @dojo.branch %></h1>
<form action="/dojo/<%= @dojo.id %>" method="post">
    <input type="hidden" name="_method" value="patch">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <h2>Branch</h2><input type="text" name="branch" value="<%= @dojo.branch %>">
    <h2>Street</h2><input type="text" name="street" value="<%= @dojo.street %>">
    <h2>City</h2><input type="text" name="city" value="<%= @dojo.city %>">
    <h2>State</h2><input type="text" name="state" value="<%= @dojo.state %>">
    <input type='submit' value='Update <%= @dojo.branch %>'>
</form>

