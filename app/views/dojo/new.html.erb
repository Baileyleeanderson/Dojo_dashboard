<!DOCTYPE html>
<html>
<head>
  <title>DojoDashboard</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<h1>Create New Dojo</h1>
<% if flash[:errors] %>
    <div class="errors"><%= flash[:errors] %></div>
  <% end %>
<form action="/dojo/create" method="post">
    <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
    <h2>Branch</h2><input type="text" name="dojo[branch]">
    <h2>Street</h2><input type="text" name="dojo[street]">
    <h2>City</h2><input type="text" name="dojo[city]">
    <h2>State</h2><input type="text" name="dojo[state]">
    <input type='submit' value='Create'>
</form>


</body>
</html>


